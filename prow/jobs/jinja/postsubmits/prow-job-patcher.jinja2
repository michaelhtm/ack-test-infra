aws-controllers-k8s/test-infra:
- name: patch-prow-jobs
  decorate: true
  annotations:
    karpenter.sh/do-not-evict: "true"
  extra_refs:
  - org:aws-controllers-k8s
    repo: test-infra
    base_ref: main
    workdir: true
  labels:
    preset-github-secrets: "true"
  spec:
    serviceAccountName: post-submit-service-account
    containers:
      - image: {{ image_context.images["patch-prow-jobs"] }}
        resources:
          limits:
            cpu: 2
            memory: "4096Mi"
          requests:
            cpu: 2
            memory: "4096Mi"
        command
